{
  "name": "nuxt-spec",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "env:auth": "node -e \"const fs=require('fs');fs.copyFileSync('.env.example','.env');console.log('.env.example -> .env');\"",
    "env:auth:ps": "powershell -NoProfile -Command \"Copy-Item -Path .\\.env.example -Destination .\\.env -Force; Write-Host '.env.example -> .env'\""
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.71",
    "@iconify-json/simple-icons": "^1.2.55",
    "@nuxt/image": "1.11.0",
    "@nuxt/scripts": "0.13.0",
    "@nuxt/ui": "^4.1.0",
    "@pinia/nuxt": "^0.11.2",
    "@unhead/vue": "^2.0.17",
    "node-appwrite": "^20.2.1",
    "nuxt": "^4.2.0",
    "pinia": "^3.0.3"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.3",
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/test-utils": "3.20.1",
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.9.1",
    "@vitest/coverage-v8": "4.0.3",
    "@vue/test-utils": "^2.4.6",
    "dotenv": "^17.2.3",
    "eslint": "^9.38.0",
    "happy-dom": "^20.0.8",
    "typescript": "^5.9.3",
    "vitest": "^4.0.3",
    "vue-tsc": "^3.1.2"
  },
  "resolutions": {
    "unimport": "4.1.1"
  },
  "packageManager": "pnpm@10.18.2"
